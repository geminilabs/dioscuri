{
  "name": "pryley/dioscuri",
  "type": "project",
  "license": "GPL-3.0+",
  "description": "WordPress boilerplate with castor and pollux",
  "homepage": "https://github.com/pryley/dioscuri/",
  "authors": [
    {
      "name": "Paul Ryley",
      "email": "paul@geminilabs.io"
    }
  ],
  "keywords": [
    "dioscuri", "pryley", "pryley", "geminilabs", "wordpress", "composer", "deployer"
  ],
  "support": {
    "issues": "https://github.com/pryley/dioscuri/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.9",
    "deployer/deployer": "^6.8",
    "johnpbloch/wordpress": "^5",
    "roots/soil": "^4.0",
    "roots/wp-stage-switcher": "^2.1",
    "wp-cli/wp-cli-bundle": "^2.4",
    "wpackagist-plugin/autodescription": "dev-trunk",
    "wpackagist-plugin/blackbar": "dev-trunk",
    "wpackagist-plugin/disable-comments": "dev-trunk",
    "wpackagist-plugin/imsanity": "dev-trunk",
    "wpackagist-plugin/machete": "dev-trunk",
    "wpackagist-plugin/members": "dev-trunk",
    "wpackagist-plugin/meta-box": "dev-trunk",
    "wpackagist-plugin/password-bcrypt": "dev-trunk",
    "wpackagist-plugin/paste-as-plain-text": "dev-trunk",
    "wpackagist-plugin/pollux": "dev-trunk",
    "wpackagist-plugin/post-type-archive-links": "dev-trunk",
    "wpackagist-plugin/pppp": "dev-trunk",
    "wpackagist-plugin/safe-redirect-manager": "dev-trunk",
    "wpackagist-plugin/safe-svg":"dev-trunk",
    "wpackagist-plugin/sendgrid-email-delivery-simplified": "dev-trunk",
    "wpackagist-plugin/simple-custom-post-order": "dev-trunk",
    "wpackagist-plugin/simple-page-sidebars": "dev-trunk",
    "wpackagist-plugin/simple-post-type-permalinks": "dev-trunk",
    "wpackagist-plugin/stops-core-theme-and-plugin-updates": "dev-trunk",
    "wpackagist-plugin/two-factor":"dev-trunk",
    "wpackagist-plugin/updraftplus": "dev-trunk",
    "wpackagist-plugin/user-menus": "dev-trunk"
  },
  "extra": {
    "installer-paths": {
      "public/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin",
        "roots/soil",
        "roots/wp-stage-switcher",
        "wpackagist-plugin/password-bcrypt"
      ],
      "public/app/plugins/{$name}/": [
        "type:wordpress-plugin",
        "wpackagist-plugin/autodescription",
        "wpackagist-plugin/blackbox",
        "wpackagist-plugin/disable-comments",
        "wpackagist-plugin/imsanity",
        "wpackagist-plugin/machete",
        "wpackagist-plugin/members",
        "wpackagist-plugin/meta-box",
        "wpackagist-plugin/paste-as-plain-text",
        "wpackagist-plugin/pollux",
        "wpackagist-plugin/post-type-archive-links",
        "wpackagist-plugin/pppp",
        "wpackagist-plugin/safe-redirect-manager",
        "wpackagist-plugin/safe-svg",
        "wpackagist-plugin/sendgrid-email-delivery-simplified",
        "wpackagist-plugin/simple-custom-post-order",
        "wpackagist-plugin/simple-page-sidebars",
        "wpackagist-plugin/simple-post-type-permalinks",
        "wpackagist-plugin/stops-core-theme-and-plugin-updates",
        "wpackagist-plugin/two-factor",
        "wpackagist-plugin/updraftplus",
        "wpackagist-plugin/user-menus"
      ]
    },
    "wordpress-install-dir": "public/wp"
  }
}
